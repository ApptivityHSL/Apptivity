<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.apptivity">

    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:usesCleartextTraffic="true"
        android:theme="@style/AppTheme">
        <activity android:name=".LoadingFromDatabase"></activity>
        <activity android:name=".PreferedTags2" />
        <activity android:name=".ActivityOverview" />
        <activity android:name=".Swiping" />
        <activity android:name=".PersonOverview" />
        <activity android:name=".Search2" />
        <activity android:name=".Search1" />
        <activity android:name=".Favourites" />
        <activity android:name=".Settings" />
        <activity android:name=".Home" />
        <activity android:name=".PersonalInformation" />
        <activity android:name=".WelcomeScreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />

                <action android:name="android.intent.action.VIEW" />
            </intent-filter>
        </activity>

        <service android:name=".FetchAddressIntentService"/>

    </application>

</manifest>